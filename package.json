{
  "name": "initai-cli",
  "version": "0.0.6",
  "description": "A suite of developer tools for creating Init.ai projects.",
  "main": "./programs/index",
  "scripts": {
    "lint": "eslint . --ignore-pattern '/test' --ignore-pattern '/node_modules' -c .eslintrc --fix",
    "test": "npm run lint && NODE_ENV=test mocha ./test --recursive",
    "test:watch": "npm t -- --watch",
    "watch": "./programs/iai-watch"
  },
  "bin": {
    "iai": "./programs/index"
  },
  "author": "Init.ai Developers <shared@init.ai>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@github.com:init-ai/initai-cli"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "emune": "1.0.0",
    "fs-extra": "0.30.0",
    "localhost.daplie.com-certificates": "1.2.0",
    "lodash.isfunction": "3.0.8",
    "natural": "0.4.0",
    "restify": "4.0.4",
    "rollbar": "0.6.2",
    "socket.io": "1.4.6",
    "walk": "2.3.9"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "chai": "3.5.0",
